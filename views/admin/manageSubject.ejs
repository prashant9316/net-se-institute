<%- include('./common/header.ejs') %>


    <div class="container-xxl flex-grow-1 container-p-y">
        <h4 class="fw-bold py-3 mb-4"><span class="text-muted fw-light">Dashboard / Subject Dashboard /</span>
            Electrical Signalling/</h4>
        <h3 class="pb-1 mb-4">Electrical Signalling | 7th Sem | ECE</h3>
        <div class="row mb-5">
            <div class="col-md-6 col-lg-4">
                <div class="card text-center mb-3">
                    <div class="card-body">
                        <h5 class="card-title">Paper</h5>
                        <div class="col-lg-12 mb-4 mb-xl-0">
                            <small class="text-light fw-semibold">Numbered</small>
                            <div class="demo-inline-spacing mt-3">
                              <ol class="list-group list-group-numbered">
                                <li class="list-group-item">Paper Year 2021 - 7th Sem</li>
                                <li class="list-group-item">Sample Paper Year 2021 | 7th Sem</li>
                                <li class="list-group-item">Paper Year 2020 - 7th Sem</li>
                                <li class="list-group-item">Sample Paper Year 2020 | 7th Sem</li>
                              </ol>
                            </div>
                          </div>
                          <div class="mb-3"></div>
                          <hr>
                          <h5>Upload More!</h5>
                          <div class="input-group">
                            <input
                              type="file"
                              class="form-control"
                              id="inputGroupFile04"
                              aria-describedby="inputGroupFileAddon04"
                              aria-label="Upload"
                            />
                            <!-- <button class="btn btn-outline-primary" type="button" id="inputGroupFileAddon04">Button</button> -->
                          </div>
                          <div class="mb-2"></div>
                        <a href="javascript:void(0)" class="btn btn-primary">Upload</a>
                    </div>
                </div>
            </div>
            <div class="col-md-6 col-lg-4">
                <div class="card text-end mb-3">
                    <div class="card-body">
                        <h5 class="card-title">Notes</h5>
                        
                        
                            <h5 class="card-header">Previously Uploaded</h5>
                            <div class="table-responsive text-nowrap">
                              <table class="table table-sm">
                                <thead>
                                  <tr>
                                    <th>Name</th>
                                    <th>View</th>
                                    <th></th>
                                  </tr>
                                </thead>
                                <tbody class="table-border-bottom-0">
                                    <% for(var i = 1; i < 4; i++) { %>
                                  <tr>
                                    <td><i class="fab fa-angular fa-lg text-danger me-3"></i> <strong>Notes Unit <%= i %></strong></td>
                                    <td><span class="badge bg-label-primary me-1">View</span></td>
                                    
                                    
                                    <td>
                                      <div class="dropdown">
                                        <button type="button" class="btn p-0 dropdown-toggle hide-arrow" data-bs-toggle="dropdown">
                                          <i class="bx bx-dots-vertical-rounded"></i>
                                        </button>
                                        <div class="dropdown-menu">
                                          <a class="dropdown-item" href="javascript:void(0);"
                                            ><i class="bx bx-edit-alt me-1"></i> Edit</a
                                          >
                                          <a class="dropdown-item" href="javascript:void(0);"
                                            ><i class="bx bx-trash me-1"></i> Delete</a
                                          >
                                        </div>
                                      </div>
                                    </td>
                                  </tr>
                                  <% } %>
                                  
                                 
                                </tbody>
                              </table>
                            </div>
                            

                            <div class="mb-3"></div>
                            <hr>
                            <h5>Upload Notes!</h5>
                            <div class="input-group">
                              <input
                                type="file"
                                class="form-control"
                                id="inputGroupFile04"
                                aria-describedby="inputGroupFileAddon04"
                                aria-label="Upload"
                              />
                              <!-- <button class="btn btn-outline-primary" type="button" id="inputGroupFileAddon04">Button</button> -->
                            </div>
                            <div class="mb-2"></div>
                          <a href="javascript:void(0)" class="btn btn-primary">Upload</a>
                    </div>
                </div>
            </div>

            <div class="col-md-6 col-lg-4">
                <div class="card mb-3">
                    <div class="card-body">
                        <h5 class="card-title">Syllabus</h5>
                        <p class="card-text">Upload</p>
                        <a href="javascript:void(0)" class="btn btn-primary">Upload Syllabus</a>
                    </div>
                </div>
            </div>

            <div class="col-md-6 col-lg-4">
                <div class="card text-end mb-3">
                    <div class="card-body">
                        <h5 class="card-title">Assignemnts</h5>
                        
                        
                            <h5 class="card-header">Previously Uploaded</h5>
                            <div class="table-responsive text-nowrap">
                              <table class="table table-sm">
                                <thead>
                                  <tr>
                                    <th>Name</th>
                                    <th>View</th>
                                    <th></th>
                                  </tr>
                                </thead>
                                <tbody class="table-border-bottom-0">
                                    <% for(var i = 1; i < 4; i++) { %>
                                  <tr>
                                    <td><i class="fab fa-angular fa-lg text-danger me-3"></i> <strong>Assignment <%= i %></strong></td>
                                    <td><span class="badge bg-label-primary me-1">View</span></td>
                                    
                                    
                                    <td>
                                      <div class="dropdown">
                                        <button type="button" class="btn p-0 dropdown-toggle hide-arrow" data-bs-toggle="dropdown">
                                          <i class="bx bx-dots-vertical-rounded"></i>
                                        </button>
                                        <div class="dropdown-menu">
                                          <a class="dropdown-item" href="javascript:void(0);"
                                            ><i class="bx bx-edit-alt me-1"></i> Edit</a
                                          >
                                          <a class="dropdown-item" href="javascript:void(0);"
                                            ><i class="bx bx-trash me-1"></i> Delete</a
                                          >
                                        </div>
                                      </div>
                                    </td>
                                  </tr>
                                  <% } %>
                                  
                                 
                                </tbody>
                              </table>
                            </div>
                            

                            <div class="mb-3"></div>
                            <hr>
                            <h5>Upload Assignemnts!</h5>
                            <div class="input-group">
                              <input
                                type="file"
                                class="form-control"
                                id="inputGroupFile04"
                                aria-describedby="inputGroupFileAddon04"
                                aria-label="Upload"
                              />
                              <!-- <button class="btn btn-outline-primary" type="button" id="inputGroupFileAddon04">Button</button> -->
                            </div>
                            <div class="mb-2"></div>
                          <a href="javascript:void(0)" class="btn btn-primary">Upload</a>
                    </div>
                </div>
            </div>

            <div class="col-md-6 col-lg-4">
                <div class="card mb-3">
                    <div class="card-body">
                        <h5 class="card-title">Announcement</h5>
                        <p class="card-text">New Announcement</p>
                        <div class="mb-3 row">
                            <label for="html5-email-input" class="col-md-12 col-form-label">Announcement</label>
                            <div class="col-md-10">
                              <input class="form-control" type="text" value="New Announcement: " id="html5-announcement" />
                            </div>
                          </div>

                        <a href="javascript:void(0)" class="btn btn-primary">Announce</a>
                    </div>
                </div>
            </div>

        </div>
    </div>

    <%- include('./common/footer.ejs') %>