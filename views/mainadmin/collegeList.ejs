<%- include('./common/header.ejs') %>






<div class="container-xxl flex-grow-1 container-p-y">
    <h4 class="fw-bold py-3 mb-4"><span class="text-muted fw-light">Dashboard /</span> Students</h4>

    <!-- Basic Bootstrap Table -->
    <div class="card">
      <h5 class="card-header">All Colleges</h5>
        
      
      <div class="table-responsive text-nowrap">
        <table class="table">
          <thead>
            <tr>
              <th>S.No.</th>
              <th>College Name</th>
              <th>College Id</th>
              <th>College Logo</th>
              <th>Actions</th>
            </tr>
          </thead>
          <tbody class="table-border-bottom-0">
              <% for(var i = 0; i < colleges.length; i++) { %>
            <tr>
                <td><small class="text-muted"><%= i+1 %></small></td>
              <td><i class="fab fa-angular fa-lg text-danger me-3"></i> <strong><%= colleges[i].collegeName %></strong></td>
              <td><%= colleges[i].collegeId %></td>
              <td>
                <ul class="list-unstyled users-list m-0 avatar-group d-flex align-items-center">
                  <!-- <li
                    data-bs-toggle="tooltip"
                    data-popup="tooltip-custom"
                    data-bs-placement="top"
                    class="avatar avatar-xs pull-up"
                    title="Lilian Fuller"
                  >
                    <img src="/assets/img/avatars/5.png" alt="Avatar" class="rounded-circle" />
                  </li>
                  <li
                    data-bs-toggle="tooltip"
                    data-popup="tooltip-custom"
                    data-bs-placement="top"
                    class="avatar avatar-xs pull-up"
                    title="Sophia Wilkerson"
                  >
                    <img src="/assets/img/avatars/6.png" alt="Avatar" class="rounded-circle" />
                  </li> -->
                  <li
                    data-bs-toggle="tooltip"
                    data-popup="tooltip-custom"
                    data-bs-placement="top"
                    class="avatar avatar-xs pull-up"
                    title="<%= colleges[i].collegeName %>"
                  >
                    <img src="<%= colleges[i].logoLink %>" alt="Avatar" class="rounded-circle" />
                  </li>
                </ul>
              </td>
              <td> <a href="/main-admin/dashboard/institute/<%= colleges[i].collegeId %>"><span class="badge bg-label-primary me-1">View</span></a></td>
              <td>
                <div class="dropdown">
                  <button type="button" class="btn p-0 dropdown-toggle hide-arrow" data-bs-toggle="dropdown">
                    <i class="bx bx-dots-vertical-rounded"></i>
                  </button>
                  <div class="dropdown-menu">
                    <a class="dropdown-item" href="javascript:void(0);"
                      ><i class="bx bx-edit-alt me-1"></i> Edit</a
                    >
                    <!-- <a class="dropdown-item" href="javascript:void(0);"
                      ><i class="bx bx-trash me-1"></i> Delete</a
                    > -->
                  </div>
                </div>
              </td>
            </tr>
            <% } %>
            
            
          </tbody>
        </table>
      </div>
    </div>

    </div>





<%- include('./common/footer.ejs') %>