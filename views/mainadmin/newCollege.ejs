  <script>

      const fetchData = (data) => {
          data.collegeName = document.getElementById('name').value;
          data.officialWebsite = document.getElementById('website').value;
          data.imageLink = document.getElementById('logoLink').value;
          console.log(data)
          return data;
      }
      const registerCollege  = async(data) => {
          
          const response = await fetch('/main-admin/dashboard/new-institute/', {
              method: "POST", 
              body: JSON.stringify(data), 
              headers: {
                  "Content-type": "application/json"
              }
          })
          .then(response => response.json())
          if(response.code == 200){
              window.location.href = "/main-admin/dashboard/new-institute/";
          } else {
              alert(response.error);
          }
          console.log(response)
          return response;
      }


      document.getElementById('new-college').addEventListener('click', e=>{
          e.preventDefault()
          data= {}
          data = fetchData(data)
          registerCollege(data)
      })
  </script>




