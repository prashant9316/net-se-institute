<%- include('./common/header.ejs') %>






<div class="container-xxl flex-grow-1 container-p-y">
    <h4 class="fw-bold py-3 mb-4"><span class="text-muted fw-light">Dashboard /</span> All Subjects</h4>

    <!-- Basic Bootstrap Table -->
    <div class="card">
      <h5 class="card-header">
        My Subjects
      </h5>
      

      <div class="table-responsive text-nowrap">
        <table class="table">
          <thead>
            <tr>
              <th>Course</th>
              <th>Subject</th>
              <!-- <th>Teachers</th> -->
              <th>Year</th>
              <th>Semester</th>
              <th>Actions</th>
            </tr>
          </thead>
          <tbody class="table-border-bottom-0">
              <% for(var i = 0; i < subjects.length; i++) { %>
            <tr>
              <td><i class="fab fa-angular fa-lg text-danger me-3"></i> <strong><%= subjects[i].courseId %></strong></td>
              <td><%= subjects[i].subjectName %></td>
              
              <td><i class="fab fa-angular fa-lg text-danger me-3"></i><%= subjects[i].year %></td>
              <td><i class="fab fa-angular fa-lg text-danger me-3"></i>Sem <%= subjects[i].sem %></td>
              <td>
                <div class="dropdown">
                  <button type="button" class="btn p-0 dropdown-toggle hide-arrow" data-bs-toggle="dropdown">
                    <i class="bx bx-dots-vertical-rounded"></i>
                  </button>
                  <div class="dropdown-menu">
                    <!-- <a class="dropdown-item" href="javascript:void(0);"
                      ><i class="bx bx-edit-alt me-1"></i> Edit</a
                    > -->
                    <a class="dropdown-item" href="/teacher/dashboard/<%= teacher.collegeId %>/manage-subject/<%= subjects[i].subjectId %>"
                      ><i class="bx bx-user me-1"></i> Manage</a
                    >
                  </div>
                </div>
              </td>
            </tr>
            <% } %>
          </tbody>
        </table>
      </div>
    </div>

    </div>




<%- include('./common/footer.ejs') %>